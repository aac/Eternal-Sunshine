{
  "name": "Eternal Sunshine",
  "version": "0.0.1",
  "description": "Hide friends from Facebook.",
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "popup.html"
  },
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["http://www.facebook.com/*"],
      "js": ["jquery-1.4.4.min.js", "eternal.js"],	     
      "run_at": "document_end"
    }
  ]
}